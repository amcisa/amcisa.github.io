extends ./layout.jade

block title
  title AmScar Voting Portal

block head
  link(href="./css/animate.min.css", rel="stylesheet")
  link(href="./css/profile.css", rel="stylesheet")
  link(href="./css/amscar.css", rel="stylesheet")


block content  
  +login_iframe()
  
  .wrapper
    #img_text.container-fluid
        h1 AmScar 2017 Current
        h4 Am斯卡最近成绩全在这里！

        .col-lg-1
        .col-lg-2
            section.container
                ul.nav.nav-pills.nav-stacked
                    li.active(style="width:175px")
                        a(href='#amcao', data-toggle='tab', aria-expanded='true') Am草
                    li
                        a(href='#amhua', data-toggle='tab', aria-expanded='true') Am花
                    li
                        a(href='#amsweetsweet', data-toggle='tab', aria-expanded='true') AmSweetSweet
                    li
                        a(href='#amtwins', data-toggle='tab', aria-expanded='true') AmTwins
                    li
                        a(href='#amsohai', data-toggle='tab', aria-expanded='true') AmSohai
                    li
                        a(href='#amshen', data-toggle='tab', aria-expanded='true') Am神
                    li
                        a(href='#amsap', data-toggle='tab', aria-expanded='true') AmSap
                    li
                        a(href='#ampattern', data-toggle='tab', aria-expanded='true') AmPattern 
                    li
                        a(href='#amdrydry', data-toggle='tab', aria-expanded='true') AmDryDry 

        .col-lg-8
            .tab-content                
                .tab-pane.fade.active.in.align-left#amcao
                    h2 Am草
                    .container-fluid.align-center
                        .col-md-6
                          img(src="uploads/amscar2017/Am草/20170327_93905_Am草_吴勇盛_豆腐.jpg")
                          h4.name 吴勇盛
                          h5 帅。由我来定义
                          .votenumber(id = "Am草_吴勇盛")
                        .col-md-6
                          img(src="uploads/amscar2017/Am草/20170317_115340_Am草_卓己安_龚象宇.jpg")
                          h4.name 龚象宇
                          h5 不用多说，看照片
                          .votenumber(id = "Am草_龚象宇")
                        .col-md-6
                          img(src="uploads/amscar2017/Am草/20170328_205935_Am草_Lee_Han_Jian_KWS.jpg")
                          h4.name Lee Han Jian
                          h5 Mister EEE cum Mister Hall 2 
                          .votenumber(id = "Am草_Lee_Han_Jian")
                .tab-pane.fade.in.align-left#amhua
                    h2 Am花
                    .container-fluid.align-center
                        .col-md-6
                            img(src="uploads/amscar2017/Am花/20170327_93825_Am花_吴勇盛_豆腐.jpg")
                            h4.name 吴勇盛
                            h5 美不需要解释多多
                            .votenumber(id = "Am花_吴勇盛")
                        .col-md-6
                            img(src="uploads/amscar2017/Am花/20170320_65156_Am花_宋玉康_静静.jpg")
                            h4.name 宋玉康
                            h5 羞涩驼顶级小鲜花一枚，盛世美颜，令人汗颜！
                            .votenumber(id = "Am花_宋玉康")
                        .col-md-6
                            img(src="uploads/amscar2017/Am花/20170319_01653_Am花_吴诗蕾_Rachel_SWL.jpg")
                            h4.name 吴诗蕾
                            h5 她是 AMCISA 刘诗诗，公认的大家姐。又漂亮又 active 的美女，大家一定要把票投给她喔
                            .votenumber(id = "Am花_吴诗蕾")
                        .col-md-6
                            img(src="uploads/amscar2017/Am花/20170331_91918_Am花_顺怡_帅哥.jpg")
                            h4.name 顺怡
                            h5 呵呵。不投我就瞪死你。呵呵。
                            .votenumber(id = "Am花_顺怡")
                        .col-md-6
                            img(src="uploads/amscar2017/Am花/20170331_92705_Am花_my_wife_and_me_yingzheng.jpg")
                            h4.name 迎祯
                            h5 请祝福我们
                            .votenumber(id = "Am花_迎祯")
                        

                .tab-pane.fade.in.align-left#amsweetsweet
                    h2 AmSweetSweet
                    .container-fluid.align-center
                        .col-md-6
                            img(src="uploads/amscar2017/AmSweetSweet/20170330_75228_AmSweetSweet_OneWay_X_彦桦_嘿嘿.jpg")
                            h4.name OneWay X 彦桦
                            h5 Bagai pinang dibelah dua
                            .votenumber(id = "AmSweetSweet_OneWay_X_彦桦")
                        .col-md-6
                            img(src="uploads/amscar2017/AmSweetSweet/20170330_75228_AmSweetSweet_Gladys_X_Shi_An_嘿嘿.jpg")
                            h4.name Gladys X Shi An
                            h5 Satu putih satu hitam
                            .votenumber(id = "AmSweetSweet_Gladys_X_Shi_An")
                        .col-md-6
                            img(src="uploads/amscar2017/AmSweetSweet/20170331_53446_AmSweetSweet_俊凯_培燊_susu.jpeg")
                            h4.name 俊凯 X 培燊
                            h5 亲爱的，有我在，你可以安心地睡
                            .votenumber(id = "AmSweetSweet_俊凯_培燊")
                        .col-md-6
                            img(src="uploads/amscar2017/AmSweetSweet/20170331_83742_AmSweetSweet_黄照财_陈冠宏_Anonymous.jpg")
                            h4.name 黄照财 X 陈冠宏
                            h5 闪瞎24K黄金狗眼
                            .votenumber(id = "AmSweetSweet_黄照财_陈冠宏")
                        .col-md-6
                            img(src="uploads/amscar2017/AmSweetSweet/20170331_92227_AmSweetSweet_Mr_Susu_and_the_Mrs_Susu_帅哥.jpg")
                            h4.name Mr Susu X Mrs Susu
                            h5 闪死人 不提怎么可以
                            .votenumber(id = "AmSweetSweet_MrSuSu_Mrs_Susu")
                .tab-pane.fade.in.align-left#amtwins
                    h2 AmTwins
                    .container-fluid.align-center
                        .col-md-6
                            img(src="uploads/amscar2017/AmTwins/20170320_63018_AmTwins_子顺_照财_静静.jpg")
                            h4.name 子顺 X 照财
                            h5 A picture is worth a thousand words~~
                            .votenumber(id = "AmTwins_子顺_照财")
                        .col-md-6
                            img(src="uploads/amscar2017/AmTwins/20170330_45713_AmTwins_德威_德威弟_郭家圣.jpg")
                            h4.name 德威 X 德威弟
                            h5 你说咧
                            .votenumber(id = "AmTwins_德威_德威弟")
                        .col-md-6
                            img(src="uploads/amscar2017/AmTwins/20170331_55329_AmTwins_培燊_李光洙_susu.jpg")
                            h4.name 培燊 X 李光洙
                            h5 这是我的双胞胎哥哥，李光洙
                            .votenumber(id = "AmTwins_培燊_李光洙")

                .tab-pane.fade.in.align-left#amsohai
                    h2 AmSohai
                    .container-fluid.align-center
                        .col-md-6
                            img(src="uploads/amscar2017/AmSohai/20170320_63259_AmSohai_牛奶妹joee_静静.jpg")
                            h4.name 牛奶妹joee
                            h5 她是我一辈子、目前为止见过最sohai的人
                            .votenumber(id = "AmSohai_牛奶妹joee")
                .tab-pane.fade.in.align-left#amshen
                    h2 Am神
                    .container-fluid.align-center
                        .col-md-6
