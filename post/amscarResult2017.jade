extends ./layout.jade

block title
  title AmScar Voting Portal

block head
  link(href="./css/animate.min.css", rel="stylesheet")
  link(href="./css/profile.css", rel="stylesheet")
  link(href="./css/amscar.css", rel="stylesheet")


block content  
  +login_iframe()
  
  .wrapper
    #img_text.container-fluid
        h1 AmScar 2016 Current
        h4 Am斯卡得主花落谁家？最近成绩全在这里！

        .col-lg-1
        .col-lg-2
            section.container
                ul.nav.nav-pills.nav-stacked
                    li.active(style="width:175px")
                        a(href='#amcao', data-toggle='tab', aria-expanded='true') Am草
                    li
                        a(href='#amhua', data-toggle='tab', aria-expanded='true') Am花
                    li
                        a(href='#amkidkid', data-toggle='tab', aria-expanded='true') AmKidKid
                    li
                        a(href='#amamam', data-toggle='tab', aria-expanded='true') AmAMAM
                    li
                        a(href='#amcouple', data-toggle='tab', aria-expanded='true') Am荧幕情侣
                    li
                        a(href='#amsleep', data-toggle='tab', aria-expanded='true') Am睡神
                    li
                        a(href='#ammoqi', data-toggle='tab', aria-expanded='true') Am默契
                    li
                        a(href='#amjaychou', data-toggle='tab', aria-expanded='true') AmJayChou 
                    li
                        a(href='#ampattern', data-toggle='tab', aria-expanded='true') AmPattern 
                    li
                        a(href='#amdrydry', data-toggle='tab', aria-expanded='true') AmDryDry 

        .col-lg-8
            .tab-content                
                .tab-pane.fade.active.in.align-left#amcao
                    h2 Am草
                    .container-fluid.align-center
                          img(src="uploads/amscar2016/Am草/颜佳煣_Rachel_Ng.jpeg")
                          h4.name 颜佳煣
                          h5 2015年5月23日 第一次看到你的时候 我觉得你好帅 想在开学的时候认识你 冥冥中却早已注定了我们的相遇相识 
                          .votenumber(id = "Am草_颜佳煣")
                .tab-pane.fade.in.align-left#amhua
                    h2 Am花
                    .container-fluid.align-center
                          img(src="uploads/amscar2016/Am花/陈伟龙_方志毅.jpg")
                          h4.name 陈伟龙
                          h5 一切纯属美丽的意外
                          .votenumber(id = "Am花_陈伟龙")

                .tab-pane.fade.in.align-left#amkidkid
                    h2 AmKidKid
                    .container-fluid.align-center
                          img(src="uploads/amscar2016/AmKidKid/纪智胜_zdank.jpg")
                          h4.name 纪智胜2
                          h5 幼稚可爱 车见车载 这么sohai的老人去哪里找
                          .votenumber(id = "AmKidKid_纪智胜2")
                .tab-pane.fade.in.align-left#amamam
                    h2 AmAMAM
                    .container-fluid.align-center
                          img(src="uploads/amscar2016/AmAMAM/OneWayTeo_郑好好.jpg")
                          h4.name OneWayTeo
                          h5 实在太干，我看不下去......
                          .votenumber(id = "AmAMAM_OneWayTeo")
                .tab-pane.fade.in.align-left#amcouple
                    h2 Am荧幕情侣
                    .container-fluid.align-center
                          img(src="uploads/amscar2016/Am荧幕情侣/长智与长智右手_郑德威.jpg")
                          h4.name 长智与长智右手
                          h5  左手只是辅助，这是一个正宫当道的年代。右手对长智意义非凡，在他们许多照片影片中记载着。虽然看起来蛮dry的，然而长智并不孤单，右手在细心的照料下茁壮的成长，不仅对他不离不弃，甚至为他任劳任怨，何等情谊！实在让我为之动容。
                          .votenumber(id = "Am荧幕情侣_长智与长智右手")

                .tab-pane.fade.in.align-left#amsleep
                    h2 Am睡神
                    .container-fluid.align-center
                          img(src="uploads/amscar2016/Am睡神/Martin_Loh__Rachel_Ng.jpeg")
                          h4.name Martin Loh
                          h5 Martin实至名归 哈哈哈 😝😝😝
                          .votenumber(id = "Am睡神_Martin_Loh")
                .tab-pane.fade.in.align-left#ammoqi
                    h2 Am默契
                    .container-fluid.align-center
                          img(src="uploads/amscar2016/Am默契/谢维杰___纪智胜_方志毅.jpg")
                          h4.name 谢维杰，纪智胜
                          h5 同样的肌肤、同样的款式、最佳搭档 
                          .votenumber(id = "Am默契_谢维杰___纪智胜")
                .tab-pane.fade.in.align-left#amjaychou
                    h2 AmJayChou
                    .container-fluid.align-center
                          img(src="uploads/amscar2016/AmJayChou/方志毅_児岛忠幸.jpg")
                          h4.name 方志毅
                          h5 我觉得被提名者全身上下带着男神气息，多才多艺，音乐·运动·摄影·成绩都是他手掌下的领域 ！ 
                          .votenumber(id = "AmJayChou_方志毅")
                .tab-pane.fade.in.align-left#ampattern
                    h2 AmPattern
                    .container-fluid.align-center
                          img(src="uploads/amscar2016/AmPattern/陈卓彦_李宜恩.jpeg")
                          h4.name 陈卓彦
                          h5 CP大人，一鸣惊人！
                          .votenumber(id = "AmPattern_陈卓彦")
                .tab-pane.fade.in.align-left#amdrydry
                    h2 AmDryDry
                          img(src="uploads/amscar2016/AmDryDry/郑释恩_戴小婷.jpg")
                          h4.name 郑释恩
                          h5 DRY了22年，就让他得奖吧！
                          .votenumber(id = "AmDryDry_郑释恩")


        .col-lg-1
                
               

block script
  script(src="js/amscarResult.js")