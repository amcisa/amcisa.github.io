mixin faq_tab(title, included)
  .container.fillx
    h1
      i.fa.fa-fw&attributes(attributes)
      | &nbsp;
      =title
    .line
    block
  br

extends ./layout.jade

block title
  title FAQ

block content
  link(href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css", rel="stylesheet")
  link(href="./css/student.css", rel="stylesheet")
  .container.fillx
    h1 升学资讯
    h4 为了帮助有意到新加坡升学的同学，下方提供了一些相关的资料供参考。 
    h4 注: 以下资料必须以各个大学为准
  .container.fillx
    .row.qa-panel-container
      .col-md-2
      .col-md-8.qa-panel.align-left.black
        h1 
          span.fa.fa-group
          | &nbsp;点击以下问题开始了解吧！
      .col-md-2
  .container.fillx.filly.align-left.qa-out

    +faq_tab("新生注意")(class="fa-life-ring yellow")
      include ../includes/checklist_faq.jade
  
    +faq_tab("申请")(class="fa-location-arrow red")
      include ../includes/registration_faq.jade
    
    +faq_tab("Tuition Grant")(class="fa-legal yellow")
      include ../includes/tuition_grant_faq.jade

    +faq_tab("银行户口， Tuition Fee Loan")(class="fa-money yellow")
      include ../includes/tuition_fee_loan_faq.jade

    +faq_tab("其他财务援助")(class="fa-info-circle red")
      include ../includes/other_loans_faq.jade

    +faq_tab("学生证明")(class="fa-credit-card green")
      include ../includes/student_pass_faq.jade

    +faq_tab("住宿")(class="fa-building blue")
      include ../includes/accomodation_faq.jade

    +faq_tab("身体检查")(class="fa-hospital-o blue")
      include ../includes/medic_exam_faq.jade

    +faq_tab("AMCISA新生营")(class="fa-fire red")
      include ../includes/amcisa_FOC_faq.jade


block script
  script(src="js/jquery-1.11.0.js")
  script(src="js/util.js")
  script(src="js/student.js")